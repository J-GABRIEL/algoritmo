#include "stdafx.h"
#include "conio.h"
#include <iostream>
using namespace std;
void main()
{
 float a,b,c,d,e,f,x,y;
 cout<<"introdusca a:";
	 cin>>a;
cout<<"introdusca b:";
	 cin>>b;
	 cout<<"introdusca c:";
	 cin>>c;
	 cout<<"introdusca d:";
	 cin>>d;
	 cout<<"introdusca e:";
	 cin>>e;
cout<<"introdusca f:";
	 cin>>f;
	 y=(a*f-c*d)/(a*e-b*d);
	 x=(f-e*y)/d;
	 cout<<"la variable x es :"<<x;
	 cout<<"la variable y es "<<y;
	 getch ();
}
